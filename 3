#include <stdio.h>

int main(){
    
    char str[50];
    char ch;
    int i,count;
    printf("Enter a string :");
    gets(str);
    i= count=0;
    while (str[i]!='\0'){
        
        ch=str[i];
   if (ch=='a'|| ch=='e'|| ch=='i'|| ch=='o'|| ch=='u'||ch=='A'|| ch=='E'|| ch=='I'|| ch=='O'|| ch=='U')
   {
       count++;
       
   }
        i++;
    }
    printf("vowels count is=%d",count);
    
    return 0;}
