#include <stdio.h>

int main(){
    
    char str[50];
    char ch;
    int i,count;
    printf("Enter a string :");
    gets(str);
    i= count=0;
    while (str[i]!='\0'){
        
        ch=str[i];
   
   {
       count++;
       
   }
        i++;
    }
    printf("string  count is=%d",count);
    
    return 0;}
